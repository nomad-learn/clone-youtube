{
  "name": "clone_juntube",
  "version": "1.0.0",
  "description": "Cloning Youtube with Vanilla and NodeJS",
  "main": "index.js",
  "author": "Juno",
  "dependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.6",
    "@babel/runtime": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "aws-sdk": "^2.601.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.0",
    "helmet": "^3.21.2",
    "mongoose": "^5.8.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.13.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-kakao": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "sass-loader": "^8.0.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --ignore 'scss'",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "dev:start": "npm run dev:server && npm run dev:assets",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore src/assets,src/static,src/webpack.config.js",
    "copyAll": "cp -R src/views build && cp -R src/static build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "start": "PRODUCTION=true node build/init.js"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
